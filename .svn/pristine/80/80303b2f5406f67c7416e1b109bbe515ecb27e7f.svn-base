.imgDiv {
    display: inline-block;
    position: relative;
}

.imgDiv .delete {
    position: absolute;
    top: 0px;
    right: 0px;
    vertical-align: top;
    width: 3px;
    height: 3px;
    display: none;
}

.fileField {
    position: relative;
    top: 0px;
    right: 0px;
    left: 5px;
    width: 80px;
    height: 60px;
    display: none;
}
th {
    border-bottom: 1px solid #d6d6d6;
}
tr:nth-child(even) {
    background: #e9e9e9;
}


