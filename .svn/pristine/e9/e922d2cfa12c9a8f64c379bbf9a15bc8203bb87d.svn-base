<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd" id="WebApp_ID" version="3.1">
  <servlet>
    <servlet-name>UploadServlet</servlet-name>
    <servlet-class>com.nrib.servlet.UploadFile</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>UploadServlet</servlet-name>
    <url-pattern>/mtsWechat/UploadServlet</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>LoginServlet</servlet-name>
    <servlet-class>com.nrib.servlet.LoginServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>LoginServlet</servlet-name>
    <url-pattern>/mtsWechat/LoginServlet</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>CodeServlet</servlet-name>
    <servlet-class>com.nrib.servlet.CodeServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>CodeServlet</servlet-name>
    <url-pattern>/mtsWechat/CodeServlet</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>LogoutServlet</servlet-name>
    <servlet-class>com.nrib.servlet.LogoutServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>LogoutServlet</servlet-name>
    <url-pattern>/mtsWechat/LogoutServlet</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>WorkListServlet</servlet-name>
    <servlet-class>com.nrib.servlet.WorkListServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>WorkListServlet</servlet-name>
    <url-pattern>/mtsWechat/WorkListServlet</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>WorkDetailServlet</servlet-name>
    <servlet-class>com.nrib.servlet.WorkDetailServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>WorkDetailServlet</servlet-name>
    <url-pattern>/mtsWechat/WorkDetailServlet</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>PageTurnServlet</servlet-name>
    <servlet-class>com.nrib.servlet.PageTurnServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>PageTurnServlet</servlet-name>
    <url-pattern>/mtsWechat/PageTurnServlet</url-pattern>
  </servlet-mapping>
  <error-page>
    <error-code>500</error-code>
    <location>/error/500.jsp</location>
  </error-page>
  <error-page>
    <error-code>404</error-code>
    <location>/error/404.jsp</location>
  </error-page>
  <filter>
    <filter-name>PermissionFilter</filter-name>
    <filter-class>com.nrib.servlet.PermissionFilter</filter-class>
  </filter>
  <filter-mapping>
    <filter-name>PermissionFilter</filter-name>
    <url-pattern>/*</url-pattern>
  </filter-mapping>
</web-app>